{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": false,
        "declaration": false,
        "removeComments": false,
        "noImplicitAny": false,
        "skipLibCheck": true,
		"rootDir":"originalTypeScripts",
        "outDir": "typeScript",
        "typeRoots": [ "./node/node_modules/@types" ],
        "types": ["node","cucumber"]
    },
    "exclude": [
        "node_modules",
        "typeScript",
		"node"
    ]
}